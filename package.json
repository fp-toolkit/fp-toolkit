{
    "name": "fp-toolkit",
    "license": "MIT",
    "author": {
        "name": "Nick Taylor",
        "email": "nick.taylor@tutamail.com"
    },
    "contributors": [
        "Ryan Johnson"
    ],
    "description": "A toolbox of useful types and functions for getting $#!7 done using a functional-first approach. Made for TypeScript.",
    "keywords": [
        "functional programming",
        "fp",
        "fp-ts",
        "utilities",
        "functional",
        "option",
        "result",
        "async"
    ],
    "version": "1.1.3",
    "main": "./dist/fp-toolkit.cjs",
    "module": "./dist/fp-toolkit.mjs",
    "types": "./dist/fp-toolkit.d.ts",
    "exports": {
        ".": {
            "require": "./dist/fp-toolkit.cjs",
            "import": "./dist/fp-toolkit.mjs"
        }
    },
    "scripts": {
        "build": "tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts",
        "test": "vitest",
        "test:coverage": "vitest --watch=false --coverage",
        "lint": "eslint . --ext .ts",
        "fmt": "prettier . --write",
        "docs": "typedoc"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.4.4",
        "@commitlint/config-conventional": "^17.4.4",
        "@typescript-eslint/eslint-plugin": "^5.48.2",
        "@typescript-eslint/parser": "^5.48.2",
        "@vitest/coverage-c8": "^0.29.2",
        "dts-bundle-generator": "^7.1.0",
        "eslint": "^8.32.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.3",
        "lint-staged": "^13.1.0",
        "prettier": "^2.8.3",
        "standard-version": "^9.5.0",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.9.1",
        "typedoc": "^0.23.27",
        "typescript": "^4.9.4",
        "vite": "^4.0.4",
        "vitest": "^0.29.2"
    }
}
